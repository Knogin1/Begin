def setup():
    size(250,250)
    
def heart():
    stroke(255,0,0)
    fill(255,0,0)
    
    rectMode(CENTER)
    translate(width / 2, height / 2)
    rotate(radians(40))
    rect(0,0,50,50)
    
    ellipseMode(CENTER)
    rotate(radians(95))
    ellipse(0,15,55,80)
    
    ellipseMode(CENTER)
    rotate(radians(-95))
    ellipse(0,-15,55,80)
    
def draw():
    heart()
